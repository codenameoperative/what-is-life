{
  "name": "what-is-life-app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "npm run --prefix web dev",
    "build": "npm run --prefix web build",
    "type-check": "npm run --prefix web type-check",
    "tauri": "npx tauri",
    "tauri:dev": "npx tauri dev",
    "tauri:build": "npx tauri build",
    "tauri:build:linux:x64": "npx tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:linux:arm64": "npx tauri build --target aarch64-unknown-linux-gnu",
    "tauri:build:linux:i686": "npx tauri build --target i686-unknown-linux-gnu",
    "tauri:build:windows": "npx tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:windows:msi": "npx tauri build --target x86_64-pc-windows-msvc && echo 'MSI build completed - ready for packaging'",
    "android:build": "echo 'Android builds handled separately'",
    "android:build:release": "echo 'Android builds handled separately'",
    "android:build:arm64": "echo 'Android builds handled separately'",
    "android:build:x86_64": "echo 'Android builds handled separately'",
    "android:build:arm32": "echo 'Android builds handled separately'",
    "android:build:x86": "echo 'Android builds handled separately'",
    "android:build:all": "echo 'Android builds handled separately'"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.6.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@phaserjs/phaser": "^0.2.2",
    "phaser": "^3.90.0"
  },
  "description": "**Notice (October 2025):** I'm back! Development of \"What is Life?\" is resuming after a brief hiatus. An exciting update is coming in late October to early november with new features and improvements. Stay tuned for more details!",
